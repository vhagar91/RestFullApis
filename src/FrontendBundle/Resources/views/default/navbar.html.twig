<div class="navbar-wrapper">
    <nav class="navbar navbar-static-home navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header page-scroll">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                {#<a class="navbar-brand" href="{{ path('homepage') }}">CBS</a>#}
              {#  <img id="logo1" style="margin-top: 8px" href="{{ path('homepage') }}" src="{{ asset('bundles/frontend/img/logo-mayor.png') }}" alt="Home" class="img-responsive">
                <img id="logo2" style="margin-top: 8px" href="{{ path('homepage') }}" src="{{ asset('bundles/frontend/img/logo-menor.png') }}" alt="Home" class="img-responsive hide">#}
            </div>
            <div id="navbar" class="navbar-collapse collapse">
               {# <div class="nav navbar-nav navbar-right">
                    {{include('FrontendBundle:language:switch.html.twig')}}
                </div>#}
                <ul class="nav navbar-nav navbar-right">
                    {{ include('FrontendBundle:default:config.html.twig') }}
                    {% if app.user is not null and (is_granted('IS_AUTHENTICATED_FULLY') or is_granted('IS_AUTHENTICATED_REMEMBERED')) %}
                        {#<li id="home-link-1"><a class="page-scroll" href="{{ path('homepage') }}">{{ 'frontendbundle.buttonhome.name'|trans }}</a></li>#}
                        <li id="home-link-0"><a class="page-scroll" href="{{ path('fos_user_security_logout') }}"><i class="fa fa-sign-out"></i>{{ 'frontendbundle.buttonlogout.name'|trans }}</a></li>
                    {% else %}
                        <li id="home-link-0"><a class="page-scroll" href=" {% if register is defined %} {{ path('user_login') }} {% else %} #login-form {% endif %}">{{ 'frontendbundle.buttonlogin.name'|trans }}</a></li>
                    {% endif %}

                    <li class="dropdown">
                        <a id="a-changeLocale" class="page-scroll no-search">
                            <select id="changeLocale" class="chosen-select-deselect form-control" onchange="window.location = this.value">
                                {% for language in language_repository.findAll() %}
                                    {% set active = "" %}
                                    {% set url = app.request.get('_route') %}
                                    {% set url_params = app.request.get('_route_params') %}
                                    {% if app.request.locale == language.code %}
                                        {% set active = 'selected=selected' %}
                                    {% endif %}
                                    <option {{ active }} value="{{ url(url, url_params|merge({'_locale':language.code})) }}">{{ language.code | upper }}</option>
                                {% endfor %}
                            </select>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
</div>